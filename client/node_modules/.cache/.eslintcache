, setPrice] = useState(0);\r\n    const [discount, setDiscount] = useState(0)\r\n\r\n    useEffect(() => {\r\n        totalAmount();\r\n    }, [cartItems]);\r\n    \r\n    const totalAmount = () => {\r\n        let price = 0, discount = 0;\r\n        cartItems.map(item => {\r\n            price += item.price.mrp\r\n            discount += (item.price.mrp - item.price.cost) \r\n        })\r\n        setPrice(price);\r\n        setDiscount(discount);\r\n    }\r\n\r\n    return (\r\n        <Box>  {/* className={classes.component}> */}\r\n            <Header>\r\n                <Heading>PRICE DETAILS</Heading>\r\n            </Header>\r\n            <Container>\r\n                <Typography>Price ({cartItems?.length} item)\r\n                    <Price component=\"span\">₹{price}</Price>\r\n                </Typography>\r\n                <Typography>Discount\r\n                    <Price component=\"span\">-₹{discount}</Price>\r\n                </Typography>\r\n                <Typography>Delivery Charges\r\n                    <Price component=\"span\">₹40</Price>\r\n                </Typography>\r\n                <TotalAmount>Total Amount\r\n                    <Price>₹{price - discount + 40}</Price>\r\n                </TotalAmount>\r\n                <Discount>You will save ₹{discount - 40} on this order</Discount>\r\n            </Container>\r\n        </Box>\r\n    )\r\n}\r\n\r\nexport default TotalView;","C:\\Users\\harsh\\Desktop\\MERN-Stack-Projects\\ECommerce-Website\\client\\src\\redux\\constants\\productConstant.js",[],[],"C:\\Users\\harsh\\Desktop\\MERN-Stack-Projects\\ECommerce-Website\\client\\src\\redux\\constants\\cartConstants.js",[],[],"C:\\Users\\harsh\\Desktop\\MERN-Stack-Projects\\ECommerce-Website\\client\\src\\Components\\Home\\Banner.jsx",[],[],"C:\\Users\\harsh\\Desktop\\MERN-Stack-Projects\\ECommerce-Website\\client\\src\\Components\\Home\\MidSlide.jsx",["215"],[],"\r\nimport { Box, styled } from '@mui/material';\r\n\r\nimport Slide from 